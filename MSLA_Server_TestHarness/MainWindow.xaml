<Window x:Class="MSLA_Server_TestHarness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <DataGrid AutoGenerateColumns="False" Height="200" HorizontalAlignment="Left" Margin="10,10,0,0" Name="dataGrid1" VerticalAlignment="Top" Width="481">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding fldAccount_ID}" Header="Account ID" />
                <DataGridTextColumn Binding="{Binding fldDate}" Header="Date" Width="80" />
                <DataGridTextColumn Binding="{Binding fldVoucher_ID}" Header="Voucher No" Width="130" />
                <DataGridTextColumn Binding="{Binding fldDebitAmt}" Header="Debits" Width="130" />
                <DataGridTextColumn Binding="{Binding fldCreditAmt}" Header="Credits" Width="130" />
            </DataGrid.Columns>
        </DataGrid>
        <Button Content="Fetch" Height="23" HorizontalAlignment="Left" Margin="10,220,0,0" Name="cmdFetch" VerticalAlignment="Top" Width="75" Click="cmdFetch_Click" />
        <TextBox Height="23" HorizontalAlignment="Left" Margin="113,219,0,0" Name="textBox1" VerticalAlignment="Top" Width="120" />
    </Grid>
</Window>
